{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ],
      "urls": [
        "https://api.football-data.org/v2/competitions",
        "https://fonts.googleapis.com/css?family=Open+Sans:400,400i,600,600i,700"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [
    {
      "name": "getCompetitions",
      "urls": [
        "https://api.football-data.org/v2/competitions"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "365d",
        "strategy": "performance"
      }
    },
    {
      "name": "get",
      "urls": [
        "https://api.football-data.org/v2/competitions/*/matches"
      ],
      "cacheConfig":{
        "maxSize": 12,
        "maxAge": "10m",
        "strategy": "performance"
      }
    },
    {
      "name": "get",
      "urls": [
        "https://api.football-data.org/v2/competitions/*/standings"
      ],
      "cacheConfig":{
        "maxSize": 12,
        "maxAge": "12h",
        "strategy": "performance"
      }
    } 
  ]
}